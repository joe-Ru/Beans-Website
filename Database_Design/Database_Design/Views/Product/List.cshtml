@model ProductListViewModel

@foreach (var product in Model.Products)
{
    @Html.Partial("ProductSummary", product)
}
<div page-model="@Model.PagingInfo" page-action="List" page-classes-enabled="true" page-class="btn" page-class-normal="btn-default" page-class-selected="btn-primary" page-url-category="@Model.CurrentCategory" class="btn-group float-right"></div>


